    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/luxon": "^1.27.0",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
