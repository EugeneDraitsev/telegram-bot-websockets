{
  "name": "telegram-bot-websockets",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/drai/telegram-bot-websockets"
  },
  "author": "Eugene Draitsev <ddrrai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env IS_LOCAL=true sls offline start",
    "build": "sls package",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "format": "prettier --write ./src",
    "tsc": "tsc --noEmit",
    "test": "cross-env IS_LOCAL=true jest --verbose",
    "logs:connect": "sls logs -t -f connect",
    "logs:disconnect": "sls logs -t -f disconnect",
    "logs:stats": "sls logs -t -f stats",
    "logs:broadcastStats": "sls logs -t -f broadcastStats",
    "logs:updateChatData": "sls logs -t -f updateChatData",
    "deploy": "sls deploy"
  },
  "dependencies": {
    "aws-sdk": "^2.1062.0",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.10",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.13",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "file-loader": "^6.2.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "serverless": "^2.72.1",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^8.3.1",
    "typescript": "^4.5.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": [
        "esbuild-jest",
        {
          "sourcemap": true
        }
      ]
    }
  }
}
