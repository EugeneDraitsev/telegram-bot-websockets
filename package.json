{
  "name": "telegram-bot-websockets",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/drai/telegram-bot-websockets"
  },
  "author": "Eugene Draitsev <ddrrai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env IS_LOCAL=true sls offline start",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "tsc": "tsc --noEmit",
    "test": "cross-env IS_LOCAL=true jest --verbose",
    "logs:connect": "sls logs -t -f connect",
    "logs:disconnect": "sls logs -t -f disconnect",
    "logs:stats": "sls logs -t -f stats",
    "logs:broadcastStats": "sls logs -t -f broadcastStats",
    "deploy": "sls deploy"
  },
  "dependencies": {
    "aws-sdk": "^2.576.0",
    "form-data": "^3.0.0",
    "lodash": "^4.17.15",
    "luxon": "^1.21.2",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.21.0",
    "@types/node": "^12.12.11",
    "@types/node-fetch": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "file-loader": "^4.3.0",
    "jest": "^24.9.0",
    "serverless": "^1.58.0",
    "serverless-offline": "^5.12.0",
    "serverless-webpack": "^5.3.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/.webpack",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "\\.mp4$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
