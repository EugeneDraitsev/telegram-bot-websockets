{
  "name": "telegram-bot-websockets",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/drai/telegram-bot-websockets"
  },
  "author": "Eugene Draitsev <ddrrai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env IS_LOCAL=true sls offline start",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "format": "prettier --write ./src",
    "tsc": "tsc --noEmit",
    "test": "cross-env IS_LOCAL=true jest --verbose",
    "logs:connect": "sls logs -t -f connect",
    "logs:disconnect": "sls logs -t -f disconnect",
    "logs:stats": "sls logs -t -f stats",
    "logs:broadcastStats": "sls logs -t -f broadcastStats",
    "logs:updateChatData": "sls logs -t -f updateChatData",
    "deploy": "sls deploy"
  },
  "dependencies": {
    "aws-sdk": "^2.1008.0",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/luxon": "^2.0.5",
    "@types/node": "^16.11.0",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.13.6",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.1.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "file-loader": "^6.2.0",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "serverless": "^2.62.0",
    "serverless-esbuild": "^1.20.0",
    "serverless-offline": "^8.2.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": [
        "esbuild-jest",
        {
          "sourcemap": true
        }
      ]
    }
  }
}
