    "eslint-plugin-jest": "^24.3.6",
    "file-loader": "^6.2.0",
    "jest": "^27.0.5",
    "prettier": "^2.3.1",
    "serverless": "^2.48.0",
    "serverless-offline": "^7.0.0",
    "serverless-webpack": "^5.5.1",
