{
  "name": "telegram-bot-websockets",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/drai/telegram-bot-websockets"
  },
  "author": "Eugene Draitsev <ddrrai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env IS_LOCAL=true sls offline start",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "tsc": "tsc --noEmit",
    "test": "cross-env IS_LOCAL=true jest --verbose",
    "logs:connect": "sls logs -t -f connect",
    "logs:disconnect": "sls logs -t -f disconnect",
    "logs:stats": "sls logs -t -f stats",
    "logs:broadcastStats": "sls logs -t -f broadcastStats",
    "logs:updateChatData": "sls logs -t -f updateChatData",
    "deploy": "sls deploy"
  },
  "dependencies": {
    "aws-sdk": "^2.751.0",
    "form-data": "^3.0.0",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/luxon": "^1.24.4",
    "@types/node": "^14.10.1",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "file-loader": "^6.1.0",
    "jest": "^26.4.2",
    "serverless": "^2.0.0",
    "serverless-offline": "^6.7.0",
    "serverless-webpack": "^5.3.4",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/.webpack",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "\\.mp4$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
