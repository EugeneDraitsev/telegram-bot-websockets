{
  "name": "telegram-bot-websockets",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/drai/telegram-bot-websockets"
  },
  "author": "Eugene Draitsev <ddrrai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env IS_LOCAL=true sls offline start",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "tsc": "tsc --noEmit",
    "test": "cross-env IS_LOCAL=true jest --verbose",
    "logs:connect": "sls logs -t -f connect",
    "logs:disconnect": "sls logs -t -f disconnect",
    "logs:stats": "sls logs -t -f stats",
    "logs:broadcastStats": "sls logs -t -f broadcastStats",
    "deploy": "sls deploy"
  },
  "dependencies": {
    "aws-sdk": "^2.518.0",
    "form-data": "^2.5.0",
    "lodash": "^4.17.15",
    "luxon": "^1.17.2",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/luxon": "^1.15.2",
    "@types/node": "^12.7.2",
    "@types/node-fetch": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "serverless": "^1.50.1",
    "serverless-offline": "^5.10.1",
    "serverless-webpack": "^5.3.1",
    "typescript": "^3.5.3",
    "webpack": "^4.39.3"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/.webpack",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "\\.mp4$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
