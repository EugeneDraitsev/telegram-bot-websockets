{
  "name": "telegram-bot-websockets",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/drai/telegram-bot-websockets"
  },
  "author": "Eugene Draitsev <ddrrai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "sls offline start",
    "build": "sls package",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "format": "prettier --write ./src",
    "tsc": "tsc --noEmit",
    "logs:connect": "sls logs -t -f connect",
    "logs:disconnect": "sls logs -t -f disconnect",
    "logs:stats": "sls logs -t -f stats",
    "logs:broadcastStats": "sls logs -t -f broadcastStats",
    "logs:updateChatData": "sls logs -t -f updateChatData",
    "deploy": "sls deploy"
  },
  "dependencies": {
    "aws-sdk": "^2.1132.0",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.14.38",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-sonarjs": "^0.13.0",
    "file-loader": "^6.2.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "serverless": "^3.17.0",
    "serverless-esbuild": "^1.26.2",
    "serverless-offline": "^8.7.0",
    "typescript": "^4.6.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": [
        "esbuild-jest",
        {
          "sourcemap": true
        }
      ]
    }
  }
}
